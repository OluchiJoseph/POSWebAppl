
@{
    ViewBag.Title = "Register";
}

<h2>Register</h2>
<h2>This is the Register page</h2>
<form method="post">
    <div class="form-group">
        <div class="col-md-4">AgentName</div>
        <div class="col-md-8"><input type="text" name="agentName" class="form-control" /></div>
    </div>
    <div class="form-group">
        <div class="col-md-4 left">WorkingArea</div>
        <div class="col-md-8 left"><input type="text" name="workingArea" class="form-control" /></div>
    </div>
    <div class="form-group">
        <div class="col-md-4 left">PhoneNumber</div>
        <div class="col-md-8 left"><input type="text" name="phoneNumber" class="form-control" /></div>
    </div>

    <div>@ViewBag.ErrorMessage</div>
    <div class="form-group">
        <div class="col-md-4"></div>
        <div class="col-md-8">
            <button type="submit" class="btn-success">
                Submit Here
            </button>
        </div>
    </div>
</form>
